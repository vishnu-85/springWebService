<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
<hibernate-mapping>  
<class name="com.restful.model.CountryDomain" table="country">  
          <id name="countryid" column="countryid">  
          <generator class="assigned"></generator>  
          </id>  
            
          <property name="countryname" column="countryname"></property>
          <property name="countrycapital" column="countrycapital"></property>
           
</class>  
            
</hibernate-mapping>
